{
  "manifest_version": 2,
  "name": "Tinder Autoswipe",
  "description": "",
  "version": "1.0",
  "icons": {
    "300": "icon.png"
  },
  "permissions": ["tabs", "activeTab", "storage", "background"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Tinder Autoswiper"
  },
  "background": {
    "scripts": ["dist/popup.bundle.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://www.tinder.com/*", "https://tinder.com/*"],
      "js": ["dist/content.bundle.js"]
    }
  ]
}
